{
    "manifest_version": 3,
    "name": "Unagent",
    "version": "0.1",
    "icons": {
      "128": "128.png"
    },
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "content_scripts": [{
        "js": ["content.js"],
        "matches": ["<all_urls>"]
    }],
    "description": "Modify the user agent reported to any website",
    "permissions": [
      "declarativeNetRequestWithHostAccess",
      "nativeMessaging"
    ],
    "host_permissions": [
      "*://*/*"
    ],
    "declarative_net_request": {
      "rule_resources": []
    }
}
